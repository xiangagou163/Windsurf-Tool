{
  "name": "windsurf-tool",
  "version": "6.4.1",
  "description": "Windsurf-Tool - 自动化管理 Windsurf 账号，支持批量注册、账号切换、到期提醒等功能",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron-vite dev",
    "build:vite": "electron-vite build",
    "build": "electron-builder",
    "build:all": "npm run build:mac && npm run build:win && npm run build:linux",
    "build:mac": "CSC_TIMESTAMP_SERVER= PATH=\"$HOME/bin:$PATH\" electron-builder --mac",
    "build:mac:x64": "CSC_TIMESTAMP_SERVER= PATH=\"$HOME/bin:$PATH\" electron-builder --mac --x64",
    "build:mac:arm64": "CSC_TIMESTAMP_SERVER= PATH=\"$HOME/bin:$PATH\" electron-builder --mac --arm64",
    "build:mac:interactive": "./build-mac.sh",
    "build:win": "electron-builder --win",
    "build:win:x64": "electron-builder --win --x64",
    "build:win:arm64": "electron-builder --win --arm64",
    "build:win:docker": "./build-docker.sh",
    "build:linux": "electron-builder --linux",
    "build:linux:x64": "electron-builder --linux --x64",
    "test:switch-with-reset": "electron test-account-switch-with-reset.js",
    "postinstall": "echo 'Installation complete'"
  },
  "keywords": [
    "windsurf",
    "account",
    "automation",
    "registration",
    "management"
